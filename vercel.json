{
  "version": 2,
  "builds": [
    {
      "src": "startup/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "staticfiles_builds"
      }
    }
  ],  "routes": [
    {
      "src": "staticfile_build/(.*)",
      "dest": "staticfile_build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "startup/wsgi.py"
    }
  ]

}
